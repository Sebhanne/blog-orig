        <form id="gform" method="POST" class="pure-form pure-form-stacked" data-email="from_email@example.com"
          action="https://script.google.com/macros/s/AKfycbwZpK_xdGQpDhbw3BJVfMDPeCJ3v9-KxSimRDRBFw/exec">
          <label for="name">Name: </label>
          <input id="name" name="name" placeholder="" />
      
          <br />
      
          <label for="message">Message: </label>
          <textarea id="message" name="message" rows="10" placeholder="Tell me anything..."></textarea>
      
          <br />
      
          <label for="email"><em>Email me</em></label>
          <input id="email" name="email" type="email" value="" required placeholder="" />
      
          <button class="button-success pure-button button-xlarge">
            <i class="fa fa-paper-plane"></i>&nbsp;Send
          </button>
        </form>
      
        <!-- Customise the Thankyou Message People See when they submit the form: -->
        <div style="display:none;" id="thankyou_message">
          <h2><em>Thanks</em> for contacting! will get back to you soon!</h2>
        </div>
      
       
